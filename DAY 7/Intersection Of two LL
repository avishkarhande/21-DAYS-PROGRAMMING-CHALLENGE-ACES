/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *getIntersectionNode(ListNode *headA, ListNode *headB) {
        ListNode* ptr1 = headA;
        ListNode* ptr2 = headB;
        while(ptr1!=NULL){
            while(ptr2!=NULL){
                if(ptr1==ptr2){
                    return ptr1;
                }
                ptr2 = ptr2->next;
            }
            ptr2 = headB;
            ptr1 = ptr1->next;
        }
        return NULL;
    }
};
